DECLARE
	vn_reg NUMBER;
BEGIN	
  FOR reg IN (SELECT no_registro
              FROM   detalle_prestamos_docs
              WHERE  no_prestamo   = :blk_v_pdd.no_prestamo
              AND    no_expediente = :blk_v_pdd.no_expediente)
  LOOP
	  vn_reg := reg.no_registro;
	  EXIT;
  END LOOP;
  IF vn_reg IS NOT NULL THEN 
  	:global.g_registro_bitacora	:= vn_reg;
    CALL_FORM('FCONGENBITACORA',NO_HIDE,DO_REPLACE);
    erase('GLOBAL.G_REGISTRO_BITACORA');
  ELSE
  	LIP_MENSAJE('Debe seleccionar un registro primero','C');
  END IF;  
END;  

