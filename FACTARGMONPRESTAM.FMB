/*Autor:                                                                */
/*Objetivo:Maximizar la ventana de la aplicación. Obtener el nombre del */
/*         usuario y desplegarlo en la barra de herramientas.           */
/*         Obtener la fecha del sistema  y desplegarlo en la barra      */
/*         de herramientas.                                             */
/*         Obtener en nombre de la forma a la que se esta accesando y   */
/*         guardar dicho nombre en una variable goblal.                 */
/*         Validar el acceso a las pantallas. Identificar la localidad  */
/*         "default" del usuario.                                       */ 
/************************************************************************/

DECLARE
   vc_forma_actual         VARCHAR2(60);
   vc_depto                cat_departamentos.no_departamento%TYPE;
   vc_nombre_depto         cat_departamentos.descripcion%TYPE;
   vn_delegacion           cat_delegaciones.no_delegacion%TYPE;
   vc_nombre_delegacion    cat_delegaciones.descripcion%TYPE;
   vn_subdelegacion        cat_subdelegaciones.no_subdelegacion%TYPE;
   vc_nombre_subdelegacion cat_subdelegaciones.descripcion%TYPE;
   vc_usuario              seg_usuarios.usuario%TYPE;
   vc_nombre_usuario       seg_usuarios.nombre%TYPE;
   vc_titulo_pantalla      seg_pantallas_aplicacion.descripcion%TYPE;
   vc_cve_pantalla         seg_pantallas_aplicacion.cve_pantalla%TYPE; 
   vc_Error                varchar2(500);
BEGIN
   -- Se ejecuta el procedimiento de librería "LIP_VALIDA_PERMISOS", encargado de activar o 
   -- desactivar los permisos de consulta, borrado, inserción y actualización de todos los 
   -- bloques de una Forma.
   LIP_VALIDA_PERMISOS;
   
   vc_forma_actual := GET_APPLICATION_PROPERTY(CURRENT_FORM_NAME);
   vc_usuario      := GET_APPLICATION_PROPERTY(USERNAME);
    
   :global.toolbar_usuario         := vc_usuario;
   :global.toolbar_no_departamento := NULL;
   :global.toolbar_forma_actual    := vc_forma_actual;
   
   pa_ini_forma(vc_usuario, vc_forma_actual, vc_nombre_usuario, vc_depto,
                vc_nombre_depto, vn_delegacion, vc_nombre_delegacion, vn_subdelegacion,
                vc_nombre_subdelegacion, vc_titulo_pantalla, vc_cve_pantalla, vc_Error);
                
   If vc_Error != '' then
      LIP_MENSAJE(vc_Error,'S');
      EXIT_FORM(NO_VALIDATE);
   End if;	

   -- Asigna los Titulos en el toolbar
   COPY(vc_usuario                    ,'blk_toolbar.toolbar_usuario');
   COPY(vc_nombre_usuario             ,'blk_toolbar.toolbar_nombre_usuario');
   COPY(TO_CHAR(SYSDATE,'DD-MM-YYYY') ,'blk_toolbar.toolbar_fecha_conexion');

   -- Asigna los Titulos en el toolbar
   COPY(vc_depto                      ,'blk_toolbar.toolbar_no_departamento');
   COPY(vc_nombre_depto               ,'blk_toolbar.toolbar_desc_departamento');
   COPY(TO_CHAR(vn_delegacion)        ,'blk_toolbar.toolbar_no_delegacion');
   COPY(vc_nombre_delegacion          ,'blk_toolbar.toolbar_desc_delegacion');
   COPY(TO_CHAR(vn_subdelegacion)     ,'blk_toolbar.toolbar_no_subdelegacion');
   COPY(vc_nombre_subdelegacion       ,'blk_toolbar.toolbar_desc_subdelegacion');

   SET_WINDOW_PROPERTY(FORMS_MDI_WINDOW,WINDOW_STATE,MAXIMIZE);
   SET_WINDOW_PROPERTY('WIN_INICIAL',WINDOW_STATE,MAXIMIZE);

   -- Obtiene la descripcion de la Pantalla para asignarsela a la Ventana Inicial
   :blk_toolbar.nombre_forma := vc_titulo_pantalla;
   :blk_toolbar.cve_forma    := vc_cve_pantalla;

   IF GET_APPLICATION_PROPERTY (CURRENT_FORM) = 'FCONGENRASTREADOR' THEN
      SET_ITEM_PROPERTY('BLK_TOOLBAR.PB_RASTREADOR_BIENES',DISPLAYED,PROPERTY_FALSE);
   END IF;    
   -- Sección donde se llama a una unidad de programa "PUP_INICIALIZA_FORMA", donde se 
   -- programará cualquier acción que se desee realizar al iniciar la ejecucion de la forma
   READ_IMAGE_FILE('Z:\Iconos\LogoSAE.jpg','JPG','ICO_SERA');
   PUP_INICIALIZA_FORMA;

END;

